{
  "name": "workshop",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node config-env.ts",
    "start": "ng serve",
    "test": "ng test",
    "build": "ng build --prod",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "predeploy": "make create_env && npm run config && npm run build && npm run sentry:deploy",
    "deploy": "serve -s dist",
    "sentry:deploy": "npm run sentry:newRelease && npm run sentry:uploadSM",
    "sentry:newRelease": "sentry-cli releases new workshop@$npm_package_version --log-level=info",
    "sentry:uploadSM": "sentry-cli releases files workshop@$npm_package_version upload-sourcemaps ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^11.2.18",
    "@angular/animations": "^11.2.14",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@sentry/angular": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "colors": "^1.4.0",
    "ngx-toastr": "^13.2.1",
    "rxjs": "^6.6.7",
    "serve": "^11.3.2",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^0.1102.18",
    "@angular/cli": "^11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@sentry/cli": "^1.71.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.18.5",
    "codelyzer": "^6.0.2",
    "dotenv": "^8.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.1.5"
  }
}
